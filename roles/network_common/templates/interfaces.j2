# The loopback network interface
auto lo
iface lo inet loopback
   address {{ ansible_hostname["interfaces"]["lo"] }}


auto eth0
iface eth0 inet dhcp
 vrf mgmt

auto mgmt
iface mgmt
  address 127.0.0.1/8
  vrf-table auto

{% for interface in ansible_hostname["ospf"] %}
auto {{interface}}
iface {{interface}}
   mtu 9216
{% endfor %}
